BUNSEN_LOG_LEVEL=info
BUNSEN_HOST=travis-ci
BUNSEN_EXECUTION_ENVIRONMENT=test
BUNSEN_SERVICE_PORT=4567
BUNSEN_SERVICE_VERSION=/v1
BUNSEN_VERSION=0.0.1

BUNSEN_CVE_URL="https://s3.amazonaws.com/s3-ion-test/files/cve/"
BUNSEN_CPE_URL="https://s3.amazonaws.com/s3-ion-test/files/cpe/dictionary/"
BUNSEN_CPE_SCHEMA_VERSION=2.3

BUNSEN_AWS_REGION=us-east-1
BUNSEN_S3_BUCKET=guard-ion-test-inside

BUNSEN_DATABASE_ADAPTER=postgresql
BUNSEN_DATABASE_DATABASE=bunsen
BUNSEN_DATABASE_PASSWORD=postgres
BUNSEN_DATABASE_HOST=postgres
BUNSEN_DATABASE_USER=postgres
BUNSEN_DATABASE_POOL=5

BUNSEN_SSL_USE=false
BUNSEN_SSL_CABUNDLE=/etc/ssl/certs/ca-certificates.crt
BUNSEN_SERVICES_URL=http://bunsen:4567/v1
